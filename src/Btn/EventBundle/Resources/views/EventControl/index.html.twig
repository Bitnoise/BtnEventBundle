{% extends 'BtnAdminBundle:Crud:index.html.twig' %}

{% block list_table_head_columns %}
    <th>{% trans %}btn_event.event.title{% endtrans %}</th>
    <th>{% trans %}btn_event.event.from_date{% endtrans %}</th>
    <th>{% trans %}btn_event.event.to_date{% endtrans %}</th>
    <th>{% trans %}btn_event.event.is_active{% endtrans %}</th>
{% endblock %}

{% block list_table_body_columns %}
    <td>{{ entity.title }}</td>
    <td>{{ entity.fromDate|date(btn_admin.list.date_time_format) }}</td>
    <td>{{ entity.toDate|date(btn_admin.list.date_time_format) }}</td>
    <td>{{ entity.isActive }}</td>
{% endblock %}
